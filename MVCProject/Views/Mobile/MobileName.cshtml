@model List<string>
@{
    string title = "MobileList";
}
<h1>@title</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Sr.No</th>
            <th>Mobile Name</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>@Model[i]</td>
            </tr>
        }
    </tbody>
</table>
